source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.0'
inhibit_all_warnings!
use_frameworks!


def application_pods
    pod 'SnapKit', :git => 'https://github.com/SnapKit/SnapKit.git', :branch => 'swift-4'
    pod 'SwiftyJSON', '3.1.4'
end

target 'WeatherCloud' do
    application_pods
end
